@namespace BlazorCharts
@inherits ChartElement<TData>
@typeparam TData

@if (Chart.IsInit)
{
    <g class="bc-title" transform="translate(@((Rect.LT+PaddingRect.LT).ToPoint()))">


        <text x="@TitleX" y="@TitleY" text-anchor="@TextAnchor.ToDescription()" font-size="@FontSize">@Title</text>

    </g>
    @*<polyline points="@Rect.ToPoints()" style="fill:rgb(0 0 255 / 0.50);" />*@
}
